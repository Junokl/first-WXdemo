.container{
    background-color:#fff;
}
.new-search-nav {
    position: fixed;
    top: 0;
    z-index: 10;
    left: 0;
    width: 100%;
    background-color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100rpx;
    box-sizing: border-box;
    padding: 0 24rpx;
}
.new-location {
    color: #000;
    font-size: 30rpx;
    margin-right: 20rpx;
    display: inline-flex;
    justify-content: center;
    align-items: center;
}
.new-location image {
    width: 36rpx;
    height: 36rpx;
    display: inline-block;
    margin-right: 10rpx;
}
.new-search {
    height: 60rpx;
    flex: 1;
    background-color: #fff;
    color: #999;
    font-size: 24rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 30rpx;
    box-shadow:0px 2rpx 12rpx 0px rgba(0, 0, 0, 0.1);
}
.new-search text {
    line-height: 60rpx;
    margin-left: 10rpx;
}
.new-search image {
    width: 32rpx;
    height: 32rpx;
}
.new-message {
    width: 48rpx;
    height: 48rpx;
    margin-left: 20rpx;
}
.new-message image {
    width: 100%;
    height: 100%;
}
.search-box{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 88rpx;
    z-index: 10;
    background: linear-gradient(rgba(0,0,0,.2),rgba(255,255,255,0)); 
}
.logo{
    width:48rpx;
    height:48rpx;
    margin:0 18rpx;
    border-radius: 50%;
    overflow: hidden;
}
.search-inner{
    display: flex;
    align-items: center;
    margin:15rpx 30rpx;
    width:675rpx;
    height: 58rpx;
    border-radius:29rpx;
    background-color: #fff;
}
.search-img{
    margin-right:12rpx;
    width: 30rpx;
    height: 30rpx;
    margin-top: -6rpx;
}
.search-cont{
    height: 58rpx;
    width:536rpx;
    padding-left: 12rpx;
    line-height: 58rpx;
    font-size: 24rpx;
    color: #666;
}
.search-fixed{
    background:rgba(242,48,48,0.8);
}
.swiper_box {
    width: 690rpx;
    height: 300rpx;
    margin: 24rpx auto;
    box-shadow:0rpx 4rpx 12rpx 0rpx rgba(0, 0, 0, 0.08);
    background-size: cover;
}
swiper-item image {
    width: 100%;
    height: 300rpx;
    display: inline-block;
    overflow: hidden;
    background-size: cover;
    border-radius: 10rpx;
}
.venues_list{
    background-color: #fff;
    padding: 10rpx 0;
    overflow: hidden;
    margin: 0 24rpx;
    border-radius: 10rpx;
}
.venues_item {
    display: block;
    float: left;
    width: 25%;
    margin-bottom: 0rpx;
    color: #666;
    font-size: 0;
    text-align: center;
}
.venues_item image{
    width: 120rpx;
    height: 120rpx;
}
.venues_item view{
    height: 28rpx;
    font-size: 26rpx;
    line-height: 28rpx;
    overflow: hidden;
}
.venues_img{
  width:690rpx;
  margin:0 auto 20rpx;
  height:240rpx;
  display: block;
}
.venues_img image{
    width: 100%;
    height: 240rpx;
}
.section{
    margin-top: 20rpx;
    background-color: #fff;
}
.section-title{
    padding-top:30rpx;
    height:54rpx;
    line-height:54rpx;
    font-size:28rpx;
    font-weight: 700;
    color: #333;
    text-align: center;
    letter-spacing: 3rpx;
}
.section-subtitle{
    line-height:0;
    width:340rpx;
    height:16rpx;
    padding-bottom: 20rpx;
    margin:0 auto;
}
.choice_box {
    font-size: 12px;
}
.choice_box .choice_list .choice_item{
    width: 368rpx;
    float: left;
    margin: 2rpx;
    background-color: #fff;
}
.choice_box .choice_list image {
    width: 100%;
    height: 368rpx;
}
.choice_box .choice_list .title {
    height: 72rpx;
    line-height: 36rpx;
    font-weight: bold;
    text-align: center;
    color: #333;
}
.choice_box .choice_list .choice_footer {
    display: block; 
    margin: 20rpx 5rpx 15rpx;
    position: relative;
    height: 30rpx;
    line-height: 17rpx;
}
.choice_box .choice_list .choice_footer .price {
  color: #ff4f70;
}
.choice_box .choice_list .choice_footer .similer {
    position: absolute;
    bottom: 0;
    right: 0;
    text-align: center;
    color: #686868;
    width: fit-content;
    line-height: 30rpx;
    border: 1rpx solid #bfbfbf;
    padding: 1rpx 6rpx;
    border-radius: 5rpx;
}
/*秒杀*/
.seckill{
    margin-top: 20rpx;
    padding: 0 30rpx 30rpx;
}
.seckill-time{
    display: flex;
    justify-content: space-between;
    align-items:center;
    height: 72rpx;
    color: #333;
}
.sk-tips{
    font-size: 36rpx;
    margin-right: 20rpx;
}
.sk-time{
    position: relative;
    display: inline-block;
    font-size: 20rpx;
    width: 30rpx;
    line-height: 30rpx;
    margin-right: 12rpx;
    color: #fff;
    border-radius: 4rpx;
    text-align: center;
}
.sk-time::before{
    content:':';
    position: absolute;
    right: -9rpx;
    top: 0;
    color: #333;
    font-weight: bold;
}
.sk-time:last-child::before{
    content: '';
}
.seckill-time>navigator{
    font-size: 28rpx;
}
.sk-pic{
    width: 28rpx;
    height: 28rpx;
    vertical-align: top;
}
.seckill-list{
    display: flex;
    overflow-x: scroll;
    overflow-y: hidden;
    padding-bottom: 10rpx;
}
.seckill-list navigator{
    width: 160rpx;
    border-radius: 16rpx;
    background-color: #fff;
}
.seckill-list navigator+navigator {
    margin-left: 30rpx;
}
.seckill-pic {
    height: 34rpx;
    width: 142rpx;
    margin-right: 10rpx;
}
.seckill-list image{
    width: 160rpx;
    height: 160rpx;
}
.seckill-list .seckill-price{
    line-height: 1.2;
    overflow: hidden;
    font-size: 34rpx;
}
.search-text {
    height: 58rpx;
    width: 536rpx;
    padding-left: 12rpx;
    line-height: 58rpx;
    font-size: 24rpx;
    color: #666;
}
.classname {
    display: flex;
    align-items: center;
}
.new-title {
    padding-top: 10rpx;
    font-size: 36rpx;
    color: #333;
    line-height: 2;
}
.new-containter {
    padding: 0 24rpx;
}
.new-product-list {
    overflow: hidden;
}
.new-product-item {
    float: left;
    width: 344rpx;
    margin-right: 14rpx;
    margin-bottom: 14rpx;
    background-color: #fff;
    border-radius: 14rpx;
    overflow: hidden;
}
.new-product-item image {
    width: 344rpx;
    height: 320rpx;
}
.new-product-text {
    padding: 15rpx;
}
.new-product-text text {
    color: #333;
    font-size: 28rpx;
    line-height: 32rpx;
    margin-bottom: 25rpx;
    height: 64rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.new-product-item:nth-child(2n) {
    margin-right: 0;
}
.new-product-footer {
    overflow: hidden;
    display: flex;
    justify-content: center;
    align-items: center;
}
.new-product-footer .price {
    color: #FF1726;
    flex: 1;
    font-size: 36rpx;
}
.new-product-footer .similer {
    height: 40rpx;
    line-height: 40rpx;
    width: 120rpx;
    color: #FF535E;
    font-size: 22rpx;
    border-radius: 20rpx;
    background-color: #FFE6EA;
    text-align: center;
}
.poste_box{
    width: 100%;
    margin: auto;
    background-color: #fff;
    border: 1px solid #000;
}
.newyear-activity-box {
    position: relative;
}

.newyear-activity-item {
    width: 338rpx;
    height: 168rpx;    
}
.newyear-activity-item image {
    width: 338rpx;
    height: 168rpx;
}
.new-ad-wapper {
    position: relative;
    overflow: hidden;
}
.new-ad-item {
    float: left;
    width: 340rpx;
    height: 166rpx;
    margin-bottom: 12rpx;
}
.new-ad-item image {
    width: 340rpx;
    height: 166rpx;
}
.new-ad-item:nth-child(1) {
    width: 338rpx;
    height: 344rpx;
    margin-bottom: 0;
    margin-right: 12rpx;
}
.new-ad-item:nth-child(1) image {
    width: 338rpx;
    height: 344rpx;
}
.new-ad-item:nth-child(3) {
    margin-bottom: 0;
}
.new-nianhuo-banner {
    width: 750rpx;
    height: 190rpx;
}
.new-nianhuo-banner image {
    width: 750rpx;
    height: 190rpx;
}